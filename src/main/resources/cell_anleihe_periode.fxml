<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" type="javafx.scene.control.ListCell"
         xmlns="http://javafx.com/javafx/11.0.1">
    <graphic>
        <HBox spacing="10.0">
            <children>
                <Label fx:id="lblName" text="Bezeichnung" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label fx:id="lblSpread" text="Spread in BP:"/>
                <TextField fx:id="txtSpread" promptText="Kurs"/>
                <Label fx:id="lblKursInsolvenz" text="Insolvenzkurs in %:"/>
                <TextField fx:id="txtKursInsolvenz" promptText="Insolvenzkurs"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </HBox>
    </graphic>
</fx:root>
