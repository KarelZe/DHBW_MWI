<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="144.0" prefWidth="622.0"
      xmlns="http://javafx.com/javafx/8.0.172-ea" fx:controller="de.dhbw.karlsruhe.controller.SpielVerwaltenController">
    <ComboBox fx:id="cmbSpiele" layoutX="22.0" layoutY="130.0" prefWidth="150.0">
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </VBox.margin>
    </ComboBox>
    <Button mnemonicParsing="false" onAction="#doSelektiertesSpielSpeichern" prefHeight="25.0" prefWidth="612.0"
            text="Spiel laden">
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </VBox.margin>
    </Button>
    <Button mnemonicParsing="false" onAction="#doSelektiertesSpielLoeschen" prefHeight="25.0" prefWidth="619.0"
            text="Ausgewähltes Spiel löschen" textAlignment="CENTER" textFill="RED">
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </VBox.margin>
    </Button>
</VBox>
