<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="144.0" prefWidth="622.0" spacing="5.0"
      xmlns="http://javafx.com/javafx/8.0.172-ea"
      fx:controller="de.dhbw.karlsruhe.controller.PeriodeVerwaltenController">
    <children>
        <Label fx:id="lblAnzahlPerioden" text="Anzahl Perioden"/>
        <TextField fx:id="txtAnzahlPerioden" promptText="Perioden"/>
        <Button defaultButton="true" mnemonicParsing="false" text="Speichern" onAction="#doSpeichern"/>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</VBox>
