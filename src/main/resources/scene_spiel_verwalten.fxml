<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="200.0"
      prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
      fx:controller="de.dhbw.karlsruhe.controller.SpielVerwaltenController">
    <ComboBox fx:id="cmbSpiele" maxWidth="1.7976931348623157E308">
        <VBox.margin>
            <Insets bottom="25.0" top="240.0"/>
        </VBox.margin>
    </ComboBox>
    <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#doSelektiertesSpielSpeichern"
            text="Spiel laden">
        <VBox.margin>
            <Insets bottom="5.0"/>
        </VBox.margin>
    </Button>
    <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#doSelektiertesSpielLoeschen"
            text="Ausgewähltes Spiel löschen" textAlignment="CENTER">
        <VBox.margin>
            <Insets bottom="10.0"/>
        </VBox.margin>
    </Button>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
