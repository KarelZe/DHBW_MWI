<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="144.0" prefWidth="622.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.dhbw.karlsruhe.controller.SpielVerwaltenController">
   <Text fx:id="header" strokeType="OUTSIDE" strokeWidth="0.0" text="Spiel auswählen ">
      <font>
         <Font name="System Bold" size="20.0" />
      </font>
      <VBox.margin>
         <Insets bottom="20.0" left="90.0" top="200.0" />
      </VBox.margin>
   </Text>
    <ComboBox fx:id="cmbSpiele" layoutX="22.0" layoutY="130.0" prefHeight="63.0" prefWidth="442.0">
        <VBox.margin>
            <Insets bottom="15.0" left="90.0" right="5.0" top="20.0" />
        </VBox.margin>
    </ComboBox>
    <Button mnemonicParsing="false" onAction="#doSelektiertesSpielSpeichern" prefHeight="25.0" prefWidth="612.0" text="Spiel laden">
        <VBox.margin>
            <Insets bottom="5.0" left="90.0" right="90.0" top="5.0" />
        </VBox.margin>
    </Button>
    <Button fx:id="btnDelete" mnemonicParsing="false" onAction="#doSelektiertesSpielLoeschen" prefHeight="25.0" prefWidth="619.0" text="Ausgewähltes Spiel löschen" textAlignment="CENTER" textFill="RED">
        <VBox.margin>
            <Insets bottom="5.0" left="90.0" right="90.0" top="5.0" />
        </VBox.margin>
    </Button>
</VBox>
