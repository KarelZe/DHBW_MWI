<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" type="javafx.scene.control.ListCell"
         xmlns="http://javafx.com/javafx/8.0.172-ea">
    <graphic>
        <HBox spacing="5.0">
            <TextField fx:id="txtName" promptText="Bezeichnung" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            <ComboBox fx:id="cmbUnternehmen" prefWidth="150.0" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
            <Button fx:id="btnLoeschen" mnemonicParsing="false" text="-" GridPane.columnIndex="3"
                    GridPane.rowIndex="1"/>
            <padding>
                <Insets bottom="2.5" left="5.0" right="5.0" top="2.5"/>
            </padding>
        </HBox>
    </graphic>
</fx:root>
